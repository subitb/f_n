{
  "welcomeFile": "/f_n/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/northwind/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "northwind",
      "csrfProtection": false
    },
    {
      "source": "^/f_n/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}